<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<VBox spacing="20.0" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/11.0.2" fx:controller="com.example.etlap.HelloController">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>
   <children>
      <HBox prefHeight="100.0" prefWidth="200.0">
         <children>
            <Button mnemonicParsing="false" text="Button" />
            <Button mnemonicParsing="false" text="Button" />
         </children>
      </HBox>
      <TableView fx:id="foodTable" minHeight="200.0" prefHeight="200.0" prefWidth="275.0">
        <columns>
          <TableColumn fx:id="colNev" minWidth="50.0" prefWidth="100.0" text="Név" />
          <TableColumn fx:id="colKat" minWidth="50.0" prefWidth="100.0" text="Kategória" />
            <TableColumn fx:id="colAr" minWidth="50.0" prefWidth="75.0" text="Ár" />
        </columns>
      </TableView>
   </children>
</VBox>
